{
  "name": "desired_languages",
  "base": "PersistedModel",
  "idInjection": true,
  "options": {
    "validateUpsert": true
  },
  "properties": {
    "language_id": {
      "type": "number",
      "required": true
    },
    "user_id": {
      "type": "number",
      "required": true
    },
    "ability": {
      "type": "number",
      "required": true,
      "default": 0
    },
    "created_at": {
      "type": "date"
    },
    "updated_at": {
      "type": "date"
    }
  },
  "validations": [],
  "relations": {
    "language": {
      "type": "belongsTo",
      "model": "languages",
      "foreignKey": "language_id"
    },
    "partners": {
      "type": "belongsTo",
      "model": "partner",
      "foreignKey": "user_id"
    },
    "abilities": {
      "type": "belongsTo",
      "model": "ability",
      "foreignKey": "ability"
    }
  },
  "acls": [],
  "methods": {}
}
