{
  "name": "partner",
  "base": "User",
  "idInjection": true,
  "forceId": true,
  "options": {
    "validateUpsert": true
  },
  "properties": {
    "cas_user": {
      "type": "string",
      "required": true
    },
    "email": {
      "type": "string",
      "required": true
    },
    "description": {
      "type": "string"
    },
    "available": {
      "type": "boolean",
      "default": true
    },
    "user_name": {
      "type": "string",
      "required": true
    },
    "gender": {
      "type": "string",
      "required": true,
      "default": "Undeclared"
    },
    "avatar_file_name": {
      "type": "string"
    },
    "avatar_content_type": {
      "type": "string"
    },
    "avatar_file_size": {
      "type": "number"
    },
    "avatar_updated_at": {
      "type": "date"
    },
    "email_conf_code": {
      "type": "string"
    },
    "email_confirmed": {
      "type": "boolean",
      "default": false
    },
    "notify_by_email": {
      "type": "boolean",
      "default": true
    },
    "affiliation": {
      "type": "string",
      "required": true,
      "default": "Not Specified"
    },
    "field_of_study": {
      "type": "string",
      "default": "Not Specified"
    },
    "created_at": {
      "type": "date",
      "required": true,
      "defaultFn": "now"
    },
    "updated_at": {
      "type": "date",
      "required": true,
      "defaultFn": "now"
    },
    "user_type": {
      "type": "boolean",
      "required": true,
      "default": false
    },
    "last_login": {
      "type": "date",
      "defaultFn": "now"
    }
  },
  "hidden": [
    "avatar_file_size",
    "avatar_content_type",
    "avatar_updated_at",
    "email_conf_code",
    "email_confirmed",
    "cas_user",
    "last_login"
  ],
  "validations": [],
  "relations": {
    "provided_languages": {
      "type": "hasMany",
      "model": "provided_languages",
      "foreignKey": "user_id"
    },
    "desired_languages": {
      "type": "hasMany",
      "model": "desired_languages",
      "foreignKey": "user_id"
    },
    "sent": {
      "type": "hasMany",
      "model": "contacts",
      "foreignKey": "sender_id"
    },
    "received": {
      "type": "hasMany",
      "model": "contacts",
      "foreignKey": "recipient_id"
    },
    "AccessToken": {
      "model": "AccessToken",
      "type": "hasMany",
      "foreignKey": "userId"
    }
  },
  "acls": [{
    "principalType": "ROLE",
    "principalId": "$everyone",
    "permission": "DENY",
    "property":"*"
  },
    {
      "principalType": "ROLE",
      "principalId": "$everyone",
      "permission": "ALLOW",
      "property": "login"
    },
    {
      "principalType": "ROLE",
      "principalId": "$everyone",
      "permission": "ALLOW",
      "property": "logout"
    },
    {
      "principalType": "ROLE",
      "principalId": "$owner",
      "permission": "ALLOW",
      "property": "findById"
    },
    {
      "principalType": "ROLE",
      "principalId": "$owner",
      "permission": "ALLOW",
      "property": "findById"
    },
    {
      "principalType": "ROLE",
      "principalId": "$owner",
      "permission": "ALLOW",
      "property": "*"
    }
  ],
  "methods": {}
}
